{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/assets/*",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css"
  }
}
